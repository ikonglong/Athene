<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<!-- SequenceDao -->
	<bean id="sequenceDao" class="com.taobao.tddl.client.sequence.impl.GroupSequenceDao" init-method="init">
		<!-- Diamond中配置的数据源应用名，必填 -->
		<property name="appName" value="ALIMEI_APP" />
		<property name="adjust" value="true"/>
		<!-- ????????? -->
<!-- 		<property name="dscount" value="1" /> -->
		<!-- group数据源列表，即指定了从哪些库中取ID起始值 -->
		<property name="dbGroupKeys">
			<list>
				<value>ALIMEI_GROUP</value>
			</list>
		</property>
		<!-- 内步长 ,默认为1000，取值在1-100000之间-->
		<property name="innerStep" value="10" />
		<!-- 重试次数，在多个gourpDataSource的场景下，建议设置成1-2次。默认为2次 -->
		<property name="retryTimes" value="2"/>
		<!-- 使用的表的表名 ，默认为sequence-->
		<property name="tableName" value="sequence" />
		<!-- id生成器的字段名,默认为name-->
		<property name="nameColumnName" value="name" />
		<!-- 存值的列的字段名,默认为value-->
		<property name="valueColumnName" value="value" />
		<!-- 存修改时间的字段名 ,默认为gmt_modified-->
		<property name="gmtModifiedColumnName" value="gmt_modified" />
	</bean>

	<!-- Sequence -->
	<bean id="aliasAccountNumberSequence" class="com.taobao.tddl.client.sequence.impl.GroupSequence"  init-method="init">
		<property name="sequenceDao" ref="sequenceDao" />
		<property name="name" value="aliasaccount_number" />
	</bean>

    <bean id="alipayBatchNoSequence" class="com.taobao.tddl.client.sequence.impl.GroupSequence"  init-method="init">
        <property name="sequenceDao" ref="sequenceDao" />
        <property name="name" value="alipay_batch_no" />
    </bean>

	
</beans>