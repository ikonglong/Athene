<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.quest.mapper.TesterMapper">

    <resultMap id="TesterMap" type="com.quest.model.Tester">
        <result column="name" property="name" />
    </resultMap>

    <select id="findByName" resultMap="TesterMap">
        select * from tester where name = #{name}
    </select>

</mapper>